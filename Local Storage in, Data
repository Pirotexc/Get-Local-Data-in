// Replace the following JSON string with the data you want to set
const jsonString = '{"key1": "value1", "key2": "value2"}';

// Parse the JSON string
const newData = JSON.parse(jsonString);

// Set the data into local storage
for (const key in newData) {
  if (newData.hasOwnProperty(key)) {
    localStorage.setItem(key, newData[key]);
  }
}

// Optional: Log a message to the console
console.log('Local storage data set successfully!');
